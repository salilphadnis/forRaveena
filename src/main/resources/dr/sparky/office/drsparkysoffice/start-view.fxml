<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="642.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dr.sparky.office.drsparkysoffice.controller.StartController">
   <ImageView fitHeight="55.0" fitWidth="50.0" layoutX="267.0" layoutY="136.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@images/logo.png" />
      </image>
   </ImageView>
   <Label layoutX="237.0" layoutY="192.0" text="Dr Sparky's Office" textFill="#616161">
      <font>
         <Font name="Arial Bold" size="13.0" />
      </font>
   </Label>
   <Label layoutX="170.0" layoutY="247.0" text="Returning User?">
      <font>
         <Font name="Arial Bold" size="22.0" />
      </font>
   </Label>
   <Label layoutX="170.0" layoutY="279.0" text="Hi, Select The appropiate login option." textFill="#545454">
      <font>
         <Font name="Arial" size="12.0" />
      </font>
   </Label>
   <Button layoutX="168.0" layoutY="311.0" mnemonicParsing="false" onAction="#patientButtonAction" prefHeight="38.0" prefWidth="254.0" style="-fx-background-color: red;" text="Patient Login" textFill="WHITE">
      <font>
         <Font name="Arial" size="13.0" />
      </font>
   </Button>
   <Button layoutX="167.0" layoutY="369.0" mnemonicParsing="false" onAction="#nurseButtonAction" prefHeight="38.0" prefWidth="254.0" style="-fx-background-color: red;" text="Nurse Login" textFill="WHITE" />
   <Button layoutX="168.0" layoutY="427.0" mnemonicParsing="false" onAction="#doctorButtonAction" prefHeight="38.0" prefWidth="254.0" style="-fx-background-color: red;" text="Doctor Login" textFill="WHITE" />
   <Label layoutX="521.0" layoutY="580.0" onMouseClicked="#signUpButtonAction" prefHeight="22.0" prefWidth="43.0" text="Sign Up" textFill="RED">
      <font>
         <Font name="Arial Bold" size="11.0" />
      </font>
   </Label>
</AnchorPane>
